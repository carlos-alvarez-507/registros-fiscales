<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <record id='rf_distritos_form' model='ir.ui.view'>
        <field name="name">rf.distritos</field>
        <field name="model">rf.distritos</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name='cia' />
                            <field name='tipo' />
                            <field name='codigo' />
                            <field name='descrip' />
                            <field name='tags' />
                        </group>
                        <group>
                            <field name='p0' />
                            <field name='p1' />
                            <field name='p2' />
                            <field name='p3' />
                            <field name='ubi' />
                        </group>
                    </group>

                </sheet>
            </form>

        </field>
    </record>


    <record id='rf_distritos_tree' model='ir.ui.view'>
        <field name="name">rf.distritos</field>
        <field name="model">rf.distritos</field>
        <field name="arch" type="xml">
            <tree>
                <field name='cia' />
                <field name='tipo' />
                <field name='codigo' />
                <field name='descrip' />
                <field name='tags' />
                <field name='p0' />
                <field name='p1' />
                <field name='p2' />
                <field name='p3' />
                <field name='ubi' />
            </tree>
        </field>
    </record>


</odoo>